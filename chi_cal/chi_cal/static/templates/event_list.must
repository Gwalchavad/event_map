<style>
    #event_list li{
        height:{{height}}px
    }    
</style>

<div class="row-fluid">
    <div class="span1">
        <div class="row-fluid" id="event_list_top_date">
            <div class="span5" id="event_list_top_month"></div>
            <div class="span5" id="event_list_top_day"></div>
        </div>
        <div class="row-fluid">
            <div class="span5">
                <ul class="nav" id="event_list_month">
                    {{#each months}}
                    <li style="height:{{height}}px">{{month}}</li>
                    {{/each}}
                </ul>
            </div>
            <div class="span5">
                <ul class="nav" id="event_list_day">
                    {{#each days}}
                    <li style="height:{{height}}px">{{day}}</li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
    <div class="span9">
    <ul id="event_list" class="nav">
      {{#each events}}
        <li class="event_item">
            <div>{{this.start_time }} - {{this.end_time }}
                <a  href="#/event/{{ id }}">{{ this.title }}</a>
            </div>
        </li>
      {{/each}}
    </ul>
    </div>
</div>
